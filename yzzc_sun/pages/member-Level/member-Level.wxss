page {
    background-color: #fff;
}

.top {
    width: 100%;
    height: 210rpx;
    background-color: #ffb62b;
    color: #fff;
    display: flex;
    align-items: center;
}

.rightMes {
    margin-right: 30rpx;
}

.userinfo-avatar {
    width: 150rpx;
    height: 150rpx;
    border-radius: 100%;
    display: flex;
    margin: auto 30rpx;
}

.userinfo-nickname {
    font-size: 30rpx;
}

.lev-wrap {
    display: flex;
    align-items: center;
    margin-top: 20rpx;
}

.vip-icon {
    width: 26rpx;
    height: 26rpx;
}

.level {
    font-size: 26rpx;
    margin-left: 10rpx;
}

.center {
    padding: 30rpx 0rpx;
    width: 100%;
    font-size: 26rpx;
}

.numRow {
    margin: 0rpx 90rpx;
    display: flex;
    justify-content: space-between;
    color: #ffb62b;
}

.progress-wrap {
    margin: 10rpx 90rpx 0rpx;
    border-radius: 5rpx;
    overflow: hidden;
}

.memberRow {
    display: flex;
    justify-content: space-between;
    margin: 20rpx 30rpx 30rpx;
}

.memberItem {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.next {
    margin: 0rpx 30rpx;
    font-size: 24rpx;
    color: #999;
}

.memWord {
    margin-top: 11rpx;
}

.origin {
    width: 20rpx;
    height: 20rpx;
    border-radius: 100%;
    background-color: #ffb62b;
    margin-bottom: -50rpx;
    z-index: 10;
}

.bottom {
    width: 100%;
    font-size: 26rpx;
    padding: 24rpx 0rpx;
}

.rule-title {
    padding: 0rpx 30rpx;
}

.rule-content {
    padding: 10rpx 30rpx;
    color: #999;
    line-height: 40rpx;
}