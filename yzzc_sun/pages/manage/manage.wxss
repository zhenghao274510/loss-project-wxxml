.head {
    display: flex;
    flex-direction: column;
    align-items: center;
    background: #ffb62b;
}

.head image {
    width: 150rpx;
    height: 150rpx;
    margin: 30rpx 0 20rpx 0;
    border-radius: 50%;
}

.head view {
    padding: 0 30rpx;
    font-size: 26rpx;
    color: #fff;
    margin-bottom: 30rpx;
}

.collect {
    margin-bottom: 20rpx;
    display: flex;
    flex-direction: row;
    background: #fff;
}

.collect-item {
    padding: 0 10rpx;
    flex: 1;
    display: flex;
    flex-direction: column;
    height: 154rpx;
    justify-content: center;
    align-items: center;
}

.collect-item>view:nth-child(1) {
    font-size: 36rpx;
    color: #FE605F;
}

.collect-item>view:nth-child(2) {
    font-size: 30rpx;
    color: #383838;
}

.data {
    background: #fff;
    padding: 0 30rpx;
}

.data-title {
    height: 78rpx;
    line-height: 78rpx;
    color: #999;
    font-size: 30rpx;
    border-bottom: 1px solid #f2f2f2;
}