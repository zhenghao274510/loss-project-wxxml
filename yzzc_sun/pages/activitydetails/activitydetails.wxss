page {
    background-color: #fff;
}

.imgs {
    width: 750rpx;
}

.container {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: left;
}

.yuding-btn {
    margin: 30rpx;
    width: 690rpx;
    height: 90rpx;
    border-radius: 10rpx;
    background-color: #ffb62b;
    color: #fff;
    font-size: 34rpx;
    text-align: center;
    line-height: 90rpx;
}

.title,.titles {
    padding: 0 15rpx;
    font-size: 30rpx;
    text-align: left;
}

.time {
    padding: 0 15rpx;
    font-size: 26rpx;
    color: #999;
    text-align: left;
}

.details {
    padding: 0 15rpx;
    font-size: 26rpx;
    color: #666;
    text-align: left;
}