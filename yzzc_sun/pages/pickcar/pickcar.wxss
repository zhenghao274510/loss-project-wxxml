.container {
    width: 100%;
}

.userMes {
    width: 100%;
    margin-top: 30rpx;
    border-radius: 10rpx;
}

.labelRow {
    height: 90rpx;
    background-color: #fff;
    margin: 0rpx 30rpx;
    border-bottom: 1rpx solid #eee;
    display: flex;
    align-items: center;
    padding: 0rpx 30rpx;
}

.input {
    color: #000;
    font-size: 26rpx;
}

.diyHolder {
    font-size: 26rpx;
    color: #999;
}

.stores-mes {
    width: 690rpx;
    display: flex;
    flex-direction: column;
    background-color: #fff;
    margin: 20rpx 30rpx 40rpx 30rpx;
    color: #000;
    border-radius: 10rpx;
}

.store {
    font-size: 30rpx;
    padding-left: 30rpx;
    padding-top: 20rpx;
}

.times {
    font-size: 26rpx;
    color: #999;
    padding-left: 30rpx;
    padding-bottom: 30rpx;
    margin-top: 6rpx;
}

.wrap {
    padding: 30rpx;
    border-top: 1rpx solid #eee;
    border-bottom: 1rpx solid #eee;
    box-sizing: border-box;
}

.storeImgs {
    float: left;
    margin-right: 10rpx;
    width: 196rpx;
    height: 130rpx;
}

.address {
    width: 100%;
    height: 85rpx;
    line-height: 85rpx;
    border-bottom: 1rpx solid #eee;
    font-size: 26rpx;
    padding-left: 30rpx;
}

.phone {
    font-size: 26rpx;
    height: 120rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    margin-left: 30rpx;
}

.select-btn {
    width: 690rpx;
    height: 90rpx;
    border-radius: 10rpx;
    background-color: #ffb62b;
    color: #fff;
    text-align: center;
    line-height: 90rpx;
}

button::after {
    border: none;
}