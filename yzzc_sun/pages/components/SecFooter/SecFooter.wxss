.sec-footer {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 98rpx;
    background: #fff;
    z-index: 1;
    border-top: 1px solid #f2f3f4;
}

.item {
    flex: 1;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.item image {
    width: 50rpx;
    height: 50rpx;
}

.choose {
    color: #ffb62b;
}