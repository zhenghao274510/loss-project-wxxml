<view class="sec-footer" style="background:{{color.foot_color}}">
    <view bindtap="_onNavTab" class="item" data-index="{{index}}" wx:if="{{item.show==1}}" wx:for="{{nav}}" wx:key="{{index}}">
        <image src="{{item.choose===0?item.img:item.imgh}}"></image>
        <text style="color:{{color.foot_font_color_one}}" wx:if="{{item.choose===0}}">{{item.txt}}</text>
        <text class="choose" style="color:{{color.foot_font_color_two}}" wx:if="{{item.choose===1}}">{{item.txt}}</text>
    </view>
</view>
