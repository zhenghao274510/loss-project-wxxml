<view class="container">
    <image class="entrBj" src="../../resource/images/manager/bj.jpg"></image>
    <form bindsubmit="bindSave">
        <view class="form-box">
            <view class="label">
                <input class="input" name="account" placeholder="请输入账号" placeholderClass="holderClass" type="number" value="{{LoginData.account}}"></input>
            </view>
            <view class="label">
                <input class="input" name="password" placeholder="请输入密码" placeholderClass="holderClass" value="{{LoginData.password}}"></input>
            </view>
            <button class="toLogin" formType="submit">登录</button>
            <view class="contact">
                <view>客服电话</view>
                <view class="conPhone">0592-12345678</view>
            </view>
        </view>
    </form>
</view>
